translate turkish ep013_lilly_talk_dd296d8a:

    # l "What?!"
    l "Ne?!"

# game/episode013_conversations.rpy:231
translate turkish ep013_lilly_talk_301aef01:

    # c "I thought you'd be happy to see me."
    c "Beni gördüğüne sevineceğini düşünmüştüm."

