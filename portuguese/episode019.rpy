    av "Ah foi?"

# game/episode019.rpy:1167
translate portuguese ep019_paclerara_aven_257fa852:

    # av "Interesting."
    av "Interessante."

# game/episode019.rpy:1168
translate portuguese ep019_paclerara_aven_bae2dad3:

