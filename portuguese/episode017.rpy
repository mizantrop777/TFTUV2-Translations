# game/episode017.rpy:1484
translate portuguese ep017_lilly_a4fe6936:

    # l "Thank you though, [p_name_short], for being so honest with me."
    l "Mas obrigada, [p_name_short], por ser honesto comigo."

# game/episode017.rpy:1485
