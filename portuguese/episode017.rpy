    ki "Ah, não se incomodem comigo."

# game/episode017.rpy:1498
translate portuguese ep017_lilly_d1c8b5a7:

    # l "No worries, I just finished taking a shower."
    l "Sem problema, eu tinha terminado o banho."

# game/episode017.rpy:1500
translate portuguese ep017_lilly_d0dbaac1:

