# game/episode022.rpy:1074
translate russian ep022_ziv_talk_23e0a2c8:

    # c "I couldn’t agree more."
    c "С этим не поспоришь."

# game/episode022.rpy:1075
translate russian ep022_ziv_talk_16799f54:

